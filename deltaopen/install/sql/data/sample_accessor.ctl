OPTIONS (errors=50,rows=50)
LOAD DATA
INFILE sample_accessor.dat
DISCARDMAX 100
REPLACE
INTO TABLE TEMP_QRULE_ACCESSOR_OPERAND
WHEN (1) != '#'
FIELDS TERMINATED BY "," OPTIONALLY ENCLOSED BY '"'
TRAILING NULLCOLS
( "ACCESSOR_ID"		FLOAT EXTERNAL,
  "UI_CATEGORY"		CHAR(20),
  "UI_NAME"		CHAR(60),
  "UI_TYPE"		CHAR(20),
  "INTERNAL_NAME"	CHAR(60),
  "JESS_TYPE"		CHAR(20),
  "JESS_ACCESSOR"	CHAR(60))
