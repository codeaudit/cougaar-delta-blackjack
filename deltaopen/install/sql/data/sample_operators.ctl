OPTIONS (errors=50,rows=50)
LOAD DATA
INFILE sample_operators.dat
DISCARDMAX 50
REPLACE
INTO TABLE TEMP_QUAL_RULE_OPERATOR
WHEN (1) != '#'
FIELDS TERMINATED BY "," OPTIONALLY ENCLOSED BY '"'
TRAILING NULLCOLS
( "OPERATOR_ID"		FLOAT EXTERNAL,
  "UI_NAME"		CHAR(60),
  "JESS_NAME"		CHAR(60),
  "OP1_TYPE"		CHAR(60),
  "OP2_TYPE"		CHAR(60))
